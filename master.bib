	keywords = {Animals Comparative Study Databases; Genetic/statistics and numerical data/*trends *Genome Genomics/methods/statistics and numerical data/trends Internet/*trends Mice Research Support; Non-U.S. Gov't},
	number = {6B},
	pages = {1501--4},
	title = {{GeneLynx mouse: integrated portal to the mouse genome}},
	volume = {13},
	year = {2003}}

