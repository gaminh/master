@article{Chalifa-Caspi:2003,
	abstract = {The interpretation of microarray expression results often includes
	extensive efforts to identify and annotate the gene representatives
	immobilised on the arrays. In this paper we describe the usage of
	our automatic GeneAnnot system, which links between Affymetrix arrays
	and the rich human gene annotations available in GeneCards. We explain
	GeneCards search options and results display; elaborate on the presentation
	of expression information in GeneCards, including both our whole-genome
	GeneNote project and external expression resources; describe the
	various parameters and displays used by GeneAnnot to assess the annotation
	quality and probeset specificity; and show how to search GeneAnnot
	and GeneNote websites directly.},
	author = {Chalifa-Caspi, V. and Shmueli, O. and Benjamin-Rodrig, H. and Rosen, N. and Shmoish, M. and Yanai, I. and Ophir, R. and Kats, P. and Safran, M. and Lancet, D.},
	date-added = {2013-03-07 16:17:28 +0000},
	date-modified = {2013-03-07 16:17:28 +0000},
	journal = {Brief Bioinform},
	keywords = {*Data Interpretation; Statistical *Oligonucleotide Array Sequence Analysis Research Support; Non-U.S. Gov't *Software},
	number = {4},
	pages = {349--60},
	title = {{GeneAnnot: interfacing GeneCards with high-throughput gene expression compendia}},
	volume = {4},
	year = {2003}}

@article{Chalifa-Caspi:2004,
	abstract = {MOTIVATION: High density oligonucleotide arrays are usually annotated
	in a one-to-one fashion, with each probeset assigned to one gene.
	However, in reality, subsets of oligonucleotides in a probeset may
	match sequences within more than one gene, potentially leading to
	misinterpretations. Moreover, a gene is often represented by more
	than one probeset, and analyzing probe matches at the mRNA level
	can help one deduce whether these probesets are derived from the
	same or different splice variants. RESULTS: The GeneAnnot system
	comprehensively documents the many-to-many relationship between oligonucleotide
	array probesets and annotated genes in GeneCards. It performs pairwise
	alignments between the probe sequences and gene transcripts, and
	assigns sensitivity and specificity scores to each probeset/gene
	pair. AVAILABILITY: http://genecards.weizmann.ac.il/geneannot/ SUPPLEMENTARY
	INFORMATION: Program description and statistics http://genecards.weizmann.ac.il/geneannot/DOC/index.html},
	author = {Chalifa-Caspi, V. and Yanai, I. and Ophir, R. and Rosen, N. and Shmoish, M. and Benjamin-Rodrig, H. and Shklar, M. and Stein, T. I. and Shmueli, O. and Safran, M. and Lancet, D.},
	date-added = {2013-03-07 16:17:28 +0000},
	date-modified = {2013-03-07 16:17:28 +0000},
	journal = {Bioinformatics},
	keywords = {*Algorithms Chromosome Mapping/*methods Comparative Study Documentation/*methods Oligonucleotide Array Sequence Analysis/*methods Research Support; Non-U.S. Gov't Sequence Alignment/*methods Sequence Analysis; DNA/methods *Software},
	number = {9},
	pages = {1457--8},
	title = {{GeneAnnot}: comprehensive two-way linking between oligonucleotide array probesets and {GeneCards} genes},
	volume = {20},
	year = {2004}}

