	journal = {Eur Respir J},
	keywords = {Carcinoma; Non-Small-Cell Lung/*genetics Carcinoma; Small Cell/*genetics Genes; ras Humans Lung Neoplasms/*genetics Oncogenes *Proto-Oncogenes Research Support; Non-U.S. Gov't Suppression; Genetic},
	number = {5},
	pages = {461--9},
	title = {{The molecular genetics of human lung cancer}},
	volume = {2},
	year = {1989}}

