	keywords = {Brain/*metabolism/radionuclide imaging Epilepsies; Partial/*metabolism/radionuclide imaging Glucose/*metabolism Humans Oxygen/metabolism Seizures/*metabolism/radionuclide imaging Temporal Lobe/metabolism},
	number = {8},
	pages = {1201--6},
	title = {{Patterns of cerebral glucose metabolism in patients with partial seizures}},
