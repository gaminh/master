@article{Li:2001tlr,
	abstract = {Tissue damage induced by infection or injury can result in necrosis,
	a mode of cell death characterized by induction of an inflammatory
	response. In contrast, cells dying by apoptosis do not induce inflammation.
	However, the reasons for underlying differences between these two
	modes of cell death in inducing inflammation are not known. Here
	we show that necrotic cells, but not apoptotic cells, activate NF-{\^I}ºB
	and induce expression of genes involved in inflammatory and tissue-repair
	responses, including neutrophil-specific chemokine genes KC and macrophage-inflammatory
	protein-2, in viable fibroblasts and macrophages. Intriguingly, NF-{\^I}ºB
	activation by necrotic cells was dependent on Toll-like receptor
	2, a signaling pathway that induces inflammation in response to microbial
	agents. These results have identified a novel mechanism by which
	cell necrosis, but not apoptosis, can induce expression of genes
	involved in inflammation and tissue-repair responses. Furthermore,
	these results also demonstrate that the NF-{\^I}ºB/Toll-like receptor
	2 pathway can be activated both by exogenous microbial agents and
	endogenous inflammatory stimuli.},
	author = {Li, Ming and Carpio, David F. and Zheng, Ye and Bruzzo, Peter and Singh, Veena and Ouaaz, Fateh and Medzhitov, Ruslan M. and Beg, Amer A.},
	date-added = {2013-06-13 05:14:49 +0000},
	date-modified = {2013-06-13 05:14:49 +0000},
	journal = {The Journal of Immunology},
	number = {12},
	pages = {7128--7135},
	title = {{An Essential Role of the {NF}-kappa {B}/{Toll-Like Receptor} pathway in Induction of Inflammatory and Tissue-Repair Gene Expression by Necrotic Cells}},
	volume = {166},
	year = {2001}}

