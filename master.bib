	date-modified = {2013-03-07 16:17:31 +0000},
	journal = {J Nucl Med},
	keywords = {Aged Alzheimer Disease/metabolism/*radionuclide imaging Brain/metabolism/*radionuclide imaging Deoxyglucose/*analogs and derivatives/diagnostic use Female Fluorine Radioisotopes/*diagnostic use Fluorodeoxyglucose F18 Glucose/metabolism Humans Male Middle Aged Research Support; U.S. Gov't; Non-P.H.S. Research Support; U.S. Gov't; P.H.S. Sensitivity and Specificity *Stereotaxic Techniques *Tomography; Emission-Computed},
	number = {7},
	pages = {1238--48},
	title = {{A diagnostic approach in Alzheimer's disease using three-dimensional stereotactic surface projections of fluorine-18-FDG PET}},
	volume = {36},
	year = {1995}}

