	pages = {328--337},
	pmid = {18987342},
	rating = {0},
	title = {{EGFR and ADAMs cooperate to regulate shedding and endocytic trafficking of the desmosomal cadherin desmoglein 2.}},
	uri = {\url{papers2://publication/doi/10.1091/mbc.E08-04-0356}},
	url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=18987342&retmode=ref&cmd=prlinks},
	volume = {20},
